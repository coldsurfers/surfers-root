{
  "name": "@coldsurfers/ocean-road",
  "version": "1.11.3",
  "bugs": {
    "url": "https://github.com/coldsurfers/surfers-root/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coldsurfers/surfers-root"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "react-native": "src/index.ts",
  "module": "dist/index.mjs",
  "source": "src/",
  "types": "dist/index.d.ts",
  "files": ["src", "dist", "package.json"],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*.css": {
      "import": "./src/css/*.css",
      "require": "./src/css/*.css"
    },
    "./native": {
      "import": "./src/native/index.ts",
      "require": "./src/native/index.ts"
    }
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "rm -rf dist && tsdown && cp -R ./src/css ./dist",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "check": "biome check .",
    "check:fix": "biome check --write ."
  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@biomejs/biome": "1.9.4",
    "@coldsurfers/ocean-road-design-tokens": "1.10.1-alpha.2",
    "@emotion/css": "11.10.6",
    "@emotion/native": "11.11.0",
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "framer-motion": "11.11.13",
    "lucide-react": "0.469.0",
    "lucide-react-native": "0.468.0",
    "prettier": "*",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native-reanimated": "3.17.1",
    "react-native-svg": "15.11.1",
    "react-spinners": "0.14.1",
    "rollup": "^3.20.7",
    "rollup-plugin-import-css": "^3.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tsdown": "^0.11.9",
    "typescript": "5.6.3"
  },
  "peerDependencies": {
    "@coldsurfers/ocean-road-design-tokens": "1.10.1-alpha.2",
    "@emotion/css": "11.10.6",
    "@emotion/native": "11.11.0",
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "framer-motion": "11.11.13",
    "lucide-react": "0.469.0",
    "lucide-react-native": "0.468.0",
    "react": ">= 19.0.0",
    "react-dom": ">= 19.0.0",
    "react-native-reanimated": ">= 3.17.1",
    "react-native-svg": "15.11.1",
    "react-spinners": "0.14.1"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
