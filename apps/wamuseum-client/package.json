{
  "name": "@coldsurfers/wamuseum-client",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "NODE_OPTIONS='--max-old-space-size=4096' next build",
    "compile": "npx graphql-codegen-esm --config ./gql/codegen.ts",
    "postcompile": "yarn lint:fix",
    "dev": "next dev",
    "lint": "next lint",
    "lint:fix": "yarn lint --fix && prettier --write .",
    "prepare": "husky install",
    "start": "next start",
    "deploy": "pm2 start ecosystem.config.js",
    "watch": "graphql-codegen -w"
  },
  "dependencies": {
    "@apollo/client": "^3.10.4",
    "@coldsurfers/hotsurf": "workspace:*",
    "@coldsurfers/ocean-road": "workspace:*",
    "@coldsurfers/zod-schema": "workspace:*",
    "@next/font": "13.1.1",
    "@tanstack/react-table": "^8.7.6",
    "@types/node": "18.11.18",
    "@uidotdev/usehooks": "^2.4.1",
    "date-fns": "^2.29.3",
    "eslint-config-next": "13.1.1",
    "graphql": "^16.6.0",
    "next": "14.2.3",
    "next-auth": "5.0.0-beta.18",
    "react": "*",
    "react-dom": "*",
    "react-native": "*",
    "react-native-web": "*",
    "react-spinners": "*",
    "string-width": "4.2.3",
    "styled-components": "*",
    "typescript": "*",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.16.1",
    "@graphql-codegen/client-preset": "^4.2.6",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@types/react": "*",
    "@types/react-dom": "*",
    "@types/react-native": "*",
    "eslint": "*",
    "pm2": "*",
    "prettier": "*",
    "strip-ansi": "^7.1.0",
    "ts-node": "^10.9.2"
  },
  "packageManager": "yarn@4.2.2"
}
