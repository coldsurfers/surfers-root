    "react-error-boundary": "5.0.0",
    "react-hook-form": "7.48.2",
    "react-lite-youtube-embed": "2.4.0",
    "react-notion-x": "7.2.5",
    "react-slick": "^0.31.0",
    "react-spinners": "0.14.1",
    "react-tweet": "^3.2.2",
    "schema-dts": "1.1.2",
