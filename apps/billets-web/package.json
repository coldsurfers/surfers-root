    "useSubscribeButton": true,
    "useProfileFeature": true,
    "useStoreRegistrationFeature": true,
    "useSurveyFeature": true,
    "useTattooFeature": true
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.0.12",
      "@types/react-dom": "19.0.4"
    }
  }
